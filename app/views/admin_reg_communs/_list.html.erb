	<% if @admin_reg_communs.any? %>
					
    <table id="admin_list" style="width: 1000px;" title="Community" >    	    	
	    <tr align="left">
		    <th field="id" width = "20"><h4>ID</h4></th>
		    <th field="created_at" width = "20"><h4>Date</h4></th>			    		    	
		    <th field="name_first" width = "20"><h4>First Name</h4></th>
		    <th field="name_last" width = "20"><h4>Last Name</h4></th>
		    <th field="name_title" width = "50"><h4>Title</h4></th>
		    <th field="name_affiliation" width = "50"><h4>Affiliation</h4></th>
		    <th field="city" width = "20"><h4>City</h4></th>		    
		    <th field="state" width = "20"><h4>State</h4></th>
		    <th field="phone" width = "20"><h4>Phone</h4></th>		    		    
		    <th field="email" width = "50"><h4>Email</h4></th>
		</tr>
		<% @admin_reg_communs.each do |reg_commun| %>
	    <tr align="left">
		    <td width = "20"><h5><%= reg_commun.id %></h5></td>
		    <td width = "20"><h5><%= reg_commun.created_at.to_formatted_s(:db) %></h5></td>
		    <td width = "20"><h5><%= reg_commun.name_first %></h5></td>
		    <td width = "20"><h5><%= reg_commun.name_last %></h5></td>
		    <td width = "50"><h5><%= reg_commun.name_title %></h5></td>
		    <td width = "50"><h5><%= reg_commun.name_affiliation %></h5></td>
		    <td width = "20"><h5><%= reg_commun.city %></h5></td>
		    <td width = "20"><h5><%= reg_commun.state %></h5></td>
		    <td width = "20"><h5><%= reg_commun.phone %></h5></td>
		    <td width = "50"><h5><%= reg_commun.email %></h5></td>
		    
		    <td width = "20"><h5>
		    <%= link_to('View', {:action => 'show', :id => reg_commun.id}) %>		    		    	
			</h5></td>
		    <td width = "20"><h5>
			<%= button_to "Delete", {:action => 'destroy', :id => reg_commun.id }, :method => :delete, data: { confirm: "Delete this record?" } %>		  
			</h5></td>
		     			    
		</tr>
		<% end %>	    	
	</table>
	<%= will_paginate @admin_reg_communs %>

	<% else %>
	<div class="row" style="color: red;">
		<h3>No record for Community</h3>
	</div>
	<div class="row-end">&nbsp;</div>			

	<% end %>
				
