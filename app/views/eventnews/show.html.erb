<% @title = "EventNews Record" %>

	<style>
	
		#signin-page {
			height: 100%;
			min-height: 900px;
		}
	
		#signin-page #signin-page-inner {
		    width: 80%;
		    margin: 0 auto;
		}	

		div .table-signin {
			display: table;
			background-color: #DEDB3C;
			width: 880px;
			border-radius: 20px;
			border:2px solid #254520;
			box-shadow: 1px 1px 10px #888888;
			padding-top: 40px;
			padding-bottom: 40px;
			padding-left: 60px;
		}
	
		.table-signin p {
			font-family: msyh; 
			font-size: 16px;
			color: #005187;"
		}
		
	</style>


	<div id="signin-page">
		
		<header>
	
			<%= render 'share/backtotop' %>

			<div style="height: 40px;"></div>
			
		</header>
		
		
		<div id="signin-page-inner">
			
			<div class="table-signin">
				<div class="row">
					
					<div class="cell-100percent" tabindex="1">

						<!--
						-->

						<h1>EventNews:</h1>
						<br />
						<p>ID: <%= @eventnew.id %></p>
						<br />
						<p>Article ID: <%= @eventnew.article_id %></p>
						<br />
						<p>URL: <%= @eventnew.name_url %></p>
						<br />
						<p>File: <%= @eventnew.name_file %></p>
						<br />
						<p>Author: <%= @eventnew.name_author %></p>
						<br />
						<p>Admin: <%= @eventnew.name_admin %></p>
						<br />
						<p>Key Words: <%= @eventnew.key_words %></p>
						<br />
						<p>Article Date: <%= @eventnew.date_article %></p>
						<br />
						<p>Created: <%= @eventnew.created_at.to_formatted_s(:db) %></p>
						<br />
						<p>Updated: <%= @eventnew.updated_at.to_formatted_s(:db) %></p>
						<br />
						<p>Genre: <%= @eventnew.article_type %></p>
						<br />
						<p>Link Title: <%= @eventnew.linktitle %></p>
						<br />
						<p>Link Img: <%= @eventnew.linkimg %></p>
						<br />
						<p>Link Folder: <%= @eventnew.linkimg_url %></p>
						<br />
						<p>Description: <%= @eventnew.description %></p>
						<br />
						<p>Slug: <%= @eventnew.slug %></p>
												
					</div>
					
			 		<%= button_to "Edit", {:action => 'edit', :id => @eventnew.id } %> 
					
				</div>
				
			</div>
			
		</div>	
		
	</div>					
					
	<div style="height: 40px;"></div>
					
	<script>

		$(window).load( function() {
		//$(document).ready(function(){

		});
	
	</script>
