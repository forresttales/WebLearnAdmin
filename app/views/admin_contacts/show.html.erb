<% provide(:title, 'Contact') %>

	<style>
	
		#signin-page {
			height: 100%;
			min-height: 900px;
		}
	
		#signin-page #signin-page-inner {
		    width: 80%;
		    margin: 0 auto;
		}	

		div .table-signin {
			display: table;
			background-color: #E6DFC8;
			width: 800px;
			height: 400px;
			padding-top: 40px;
			padding-bottom: 40px;
			padding-left: 60px;
		}
	
		.table-signin p {
			font-family: msyh; 
			font-size: 16px;
			color: #005187;"
		}
		
	</style>


	<div id="signin-page">
		
		<header>
	
			<%= render 'share/backtotop' %>

			<div style="height: 160px;"></div>
			
		</header>
		
		
		<div id="signin-page-inner">
			
			<div class="table-signin">
				<div class="row">
					
					<div class="cell-100percent" tabindex="1">

						<h1>Contact:</h1>
						<br />
						<p>ID: <%= @admin_contact.id %></p>
						<br />
						<p>Created: <%= @admin_contact.created_at.to_formatted_s(:db) %></p>
						<br />
						<p>Name: <%= @admin_contact.name %></p>
						<br />
						<p>Email: <%= @admin_contact.email %></p>
						<br />
						<p>Subject: <%= @admin_contact.subject %></p>
						<br />
						<p>Message: <%= @admin_contact.message %></p>
												
					</div>
					
			 		<!-- = button_to "Edit", {:action => 'edit', :id => @admin_contact.id } -->  
					
				</div>
				
			</div>
			
		</div>	
		
	</div>					
					
	<div style="height: 40px;"></div>
					
	<script>

		// $(window).load( function() {
		// $(document).ready(function(){

		// });
	
	</script>
