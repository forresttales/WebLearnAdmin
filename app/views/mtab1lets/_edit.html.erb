								<div class="input-form-spacer"></div>
		
								<div class="row">
									<div class="cell-left-form">
										<%= f.label(:col_3, 'Col 3', class: 'input-label-form') %>							
									</div>
									<div class="cell-right-form">
										<%= f.text_field :col_3, :class => 'input-form', :tabindex => '2', :autofocus => true %>
