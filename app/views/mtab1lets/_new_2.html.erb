									</div>
									<div class="cell-right-form">
										
								   		<%= f.select( :col_41,
								   				[['No', '0'],								   		
								   				['Yes','1']],
								   				{}, 
								   				{ :class => 'form-select', :id => 'select_col_41', :tabindex => '43' } ) %>

									</div>
									<div class="cell-error" id="state_col_41"><p id="error_col_41"></p></div>      					   							 								      	
