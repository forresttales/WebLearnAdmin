								<div class="input-form-spacer"></div>
								<div class="input-form-spacer"></div>
								<div class="input-form-spacer"></div>
		
								<div class="row">
									<div style="width: 600px; text-align: center;">
									<%= f.submit 'Submit', :id => 'form-submit-edit', :class => 'form-btn', :tabindex => '53' %>
									</div>      					   							 	
								</div>      		

								<% end %>
								
							</div>
							
						</div>
						
					</div>
			
				</div>
				
				<div class="col-right">
				</div>												
			</div>
