									var message;
									var button;
									var cssClassDisabled = "form-button-disabled";
									var cssClassInvalid = "form-value-invalid";
									function OnLoad() {
										message = $("div#" + msgId).hide();
										form = $("div#" + formId).show();
										form.find("input[name=\"param1\"]").attr("value", window.location.host);
										form.find("input[name=\"param2\"]").attr("value", window.location.href);
										button = form.find("input[type=\"submit\"]").click(OnSubmitClick);
									}

									function OnSubmitClick() {
										var valid = true;
										var txtName = form.find("input[name=\"name\"]").removeClass(cssClassInvalid);
										var txtEmail = form.find("input[name=\"email\"]").removeClass(cssClassInvalid);
										var txtSubject = form.find("input[name=\"subject\"]").removeClass(cssClassInvalid);
										var txtMessage = form.find("textarea[name=\"message\"]").removeClass(cssClassInvalid);
										var name = $.trim(txtName.attr("value"));
										if (name.length === 0) {
											valid = false;
											txtName.addClass(cssClassInvalid);
										}
										var emailAddress = $.trim(txtEmail.attr("value"));
										if (emailAddress.length === 0) {
											valid = false;
											txtEmail.addClass(cssClassInvalid);
										} else {
											if (!regexhelper.validateEmailAddress(emailAddress)) {
												valid = false;
												txtEmail.addClass(cssClassInvalid);
											}
										}
										var subject = $.trim(txtSubject.attr("value"));
										if (subject.length === 0) {
											valid = false;
											txtSubject.addClass(cssClassInvalid);
										}
										var message = $.trim(txtMessage.val());
										if (message.length === 0) {
											valid = false;
											txtMessage.addClass(cssClassInvalid);
										}
										if (!valid) {
											return;
										}
										if (button.hasClass(cssClassDisabled)) {
											return;
										}
										button.addClass(cssClassDisabled);
										var formData = {
											param1 : form.find("input[name=\"param1\"]").attr("value"),
											param2 : form.find("input[name=\"param2\"]").attr("value"),
											param3 : form.find("input[name=\"param3\"]").attr("value"),
											param4 : form.find("input[name=\"param4\"]").attr("value"),
											param5 : form.find("input[name=\"param5\"]").attr("value"),
											param6 : form.find("input[name=\"param6\"]").attr("value"),
											name : name,
											email : emailAddress,
											subject : subject,
											message : message
										};
										var qsFormData = $.param(formData);
										var settings = {
											url : phpurl,
											data : {
												action : "post",
												options : {
													url : svcurl,
													data : qsFormData,
													proxy : 0
												}
											},
											type : "post",
											success : OnSubmitSuccess,
											error : OnSubmitError
										};
										$.ajax(settings);
									}

									function OnSubmitSuccess(response) {
										if (response.status == "error") {
											OnSubmitError();
											return;
										}
										if (postSubmitOption === 0) {
											form.hide();
											message.show();
											message.parent().css("overflow", "auto");
										} else {
											window.location = postSubmitUrl;
										}
									}

									function OnSubmitError() {
										button.removeClass(cssClassDisabled);
										alert("Unknown error occurred. Please try again.");
									}
									$(document).ready(OnLoad);
								});
