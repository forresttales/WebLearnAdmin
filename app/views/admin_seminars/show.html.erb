<% provide(:title, 'Seminar') %>

	<style>
	
		#signin-page {
			height: 100%;
			min-height: 900px;
		}
	
		#signin-page #signin-page-inner {
		    width: 80%;
		    margin: 0 auto;
		}	

		div .table-signin {
			display: table;
			background-color: #E6DFC8;
			width: 800px;
			height: 400px;
			padding-top: 40px;
			padding-bottom: 40px;
			padding-left: 60px;
		}
	
		.table-signin p {
			font-family: msyh; 
			font-size: 16px;
			color: #005187;"
		}
		
	</style>


	<div id="signin-page">
		
		<header>
	
			<%= render 'share/backtotop' %>

			<div style="height: 160px;"></div>
			
		</header>
		
		
		<div id="signin-page-inner">
			
			<div class="table-signin">
				<div class="row">
					
					<div class="cell-100percent" tabindex="1">

						<h1>Contact:</h1>
						<br />
						<p>ID: <%= @admin_seminar.id %></p>
						<br />
						<p>Created: <%= @admin_seminar.created_at.to_formatted_s(:db) %></p>
						<br />
						<p>Created: <%= @admin_seminar.updated_at.to_formatted_s(:db) %></p>
						<br />
						<p>First Name: <%= @admin_seminar.name_first %></p>
						<br />
						<p>Last Name: <%= @admin_seminar.name_last %></p>
						<br />
						<p>Title Name: <%= @admin_seminar.name_title %></p>
						<br />
						<p>Affiliation Type: <%= @admin_seminar.type_affiliation %></p>
						<br />
						<p>Affiliation: <%= @admin_seminar.name_affiliation %></p>
						<br />
						<p>Address: <%= @admin_seminar.address %></p>
						<br />
						<p>City: <%= @admin_seminar.city %></p>
						<br />
						<p>State: <%= @admin_seminar.state %></p>
						<br />
						<p>Zip: <%= @admin_seminar.zip %></p>
						<br />
						<p>Phone: <%= @admin_seminar.phone %></p>
						<br />
						<p>Email: <%= @admin_seminar.email %></p>
						<br />
						<p>Email cc 1: <%= @admin_seminar.email_cc_1 %></p>
						<br />
						<p>Email cc 2: <%= @admin_seminar.email_cc_2 %></p>
						<br />
						<p>Seminar 1: <%= @admin_seminar.seminar_1 %></p>
						<br />
						<p>Seminar 1 Text: <%= @admin_seminar.seminar_1_text %></p>
<!-- 
# Table name: seminars
#	
#  name_first       :string(50)
#  name_last        :string(50)
#  name_title       :string(50)
#  type_affiliation :string(50)
#  name_affiliation :string(100)
#  address          :string(100)
#  city             :string(50)
#  state            :string(50)
#  zip              :string(10)
#  phone            :string(50)
#  email            :string(100)
#  email_cc_1       :string(100)
#  email_cc_2       :string(100)
#  string           :string(100)
#  created_at       :datetime
#  updated_at       :datetime
#  seminar_1        :boolean          default(FALSE)
#  seminar_1_text   :string(100)
-->
												
					</div>
					
			 		<!-- = button_to "Edit", {:action => 'edit', :id => @admin_contact.id } -->  
					
				</div>
				
			</div>
			
		</div>	
		
	</div>					
					
	<div style="height: 40px;"></div>
					
