					   				['Col 17','17'],
					   				['Col 18','18'],
					   				['Col 19','19'],
					   				['Col 20','20'],
					   				['Col 21','21'],
					   				['Col 22','22'],
					   				['Col 23','23'],
					   				['Col 24','24'],					   				
					   				['Col 25','25'],					   				
					   				['Col 26','26'],					   				
					   				['Col 27','27'],					   				
					   				['Col 28','28'],					   				
					   				['Col 29','29'],					   				
					   				['Col 30','30'],					   				
					   				['Col 31','31'],					   				
					   				['Col 32','32'],					   				
					   				['Col 33','33'],					   				
					   				['Col 34','34'],					   				
					   				['Col 35','35'],					   				
					   				['Col 36','36'],					   				
					   				['Col 37','37'],					   				
					   				['Col 38','38'],					   				
					   				['Col 39','39'],					   				
					   				['Col 40','40'],					   				
					   				['Col 41','41'],					   				
					   				['Col 42','42'],					   				
					   				['Col 43','43'],					   				
					   				['Col 44','44'],					   				
					   				['Col 45','45'],					   				
					   				['Col 46','46'],					   				
					   				['Col 47','47'],					   				
					   				['Col 48','48'],					   				
					   				['Col 49','49'],					   				
					   				['Col 50','50']])) %>
							
						    </span>

							<span>
						    	<%= submit_tag "Search", :id => nil %>
						  	</span>
						  	<span><div style="display: inline-block; width: 200px;"></div></span>
							<span>
								<%= link_to('Add New Record', { :action => 'new', :form => :js }, :class => 'links', :remote => true ) %>
							</span>						  						
							<div id="tablets">
								<%= render 'mtab8lets/upload_files'%>
							</div>
						  
						<% end %>
												
					</div>
					
				</div>
				
			</div>
			<div style="height: 30px;"></div>			
			<div>
				<img src="../repository/image-mtab8lets-optout-page.png" alt="image-mtab8lets-optout-page.png">
			</div>
		</div>	
		
	</div>					

	<script>

		$(document).on('click','#tablets th a, #tablets .pagination a',function(){
		    $.getScript(this.href);
		    return false;
		});

		$("#tablets_search").submit(function() {	
			//alert('tablets_search');
		    $.get($("#tablets_search").attr("action"), $("#tablets_search").serialize(), null, "script");
		    return false;
		});
	
	</script>
					
