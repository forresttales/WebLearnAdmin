	<script type="text/javascript">
		var unfilledAdCalls = new Array();
		var application_url = "http://www.inc.com/";

		var baseURL;
		var urlParts = location.href.split('/');
		if (urlParts[urlParts.length-1] == '') {
			urlParts = urlParts.slice(0,urlParts.length-1);
		}
		if (isNaN(urlParts[urlParts.length-1].split('-').join('')) || isNaN(parseInt(urlParts[urlParts.length-1].split('-').join(''),10))) {
			baseURL = urlParts.join('/');
		}
		else {
			baseURL = urlParts.slice(0,urlParts.length-1).join('/');
		}

		window.onpageshow = function(event) {
			//reload page on back or forward
			if (event.persisted) {
				window.location.reload();
			}
		};

		// ie fix: Ensures there will be no 'console is undefined' errors
		window.console = window.console || (function(){
			var c = {}; c.log = c.warn = c.debug = c.info = c.error = c.time = c.dir = c.profile = c.clear = c.exception = c.trace = c.assert = function(){};
			return c;
		})();

		$(function(){
			if (localStorage && localStorageSupported) {
				if (!localStorage.getItem('incPinboardData')) {
					localStorage.setItem('incPinboardData',JSON.stringify(new Array()));
				}
				if (!localStorage.getItem('incPinboardSorting')) {
					localStorage.setItem('incPinboardSorting',JSON.stringify({home:'notset',channel:'notset'}));
				}
				if (!localStorage.getItem('incSponsoredTileInfo')) {
					var incSponsoredTileInfo = new Object();
					incSponsoredTileInfo.queue = new Array();
					incSponsoredTileInfo.seen = new Array();
					localStorage.setItem('incSponsoredTileInfo',JSON.stringify(incSponsoredTileInfo));
				}
				var localStorageArray = $.parseJSON(localStorage.getItem('incPinboardData'));

				var urlParts = location.href.split('/');
				if (urlParts[urlParts.length-1] == '') {
					urlParts = urlParts.slice(0,urlParts.length-1);
				}
				if (isNaN(urlParts[urlParts.length-1]) || isNaN(parseInt(urlParts[urlParts.length-1],10))) {
					var nonpushURL = urlParts.join('/');
				}
				else {
					var nonpushURL = urlParts.slice(0,urlParts.length-1).join('/');
				}

				var alreadyRecordedIndex = -1;
				for (var i = localStorageArray.length-1; i >= 0; i--) {
					if (localStorageArray[i].href == nonpushURL) {
						alreadyRecordedIndex = i;
						break;
					}
				}

				if (alreadyRecordedIndex == -1) {
					localStorageArray.push({timestamp: new Date().getTime(),href: nonpushURL});
					if (localStorageArray.length > 20) {
						localStorageArray.shift();
					}
				}
				else {
					var splicedItem = localStorageArray.splice(alreadyRecordedIndex,1)[0];
					localStorageArray.push(splicedItem);
					localStorageArray[localStorageArray.length-1].timestamp = new Date().getTime();
				}
				localStorage.setItem('incPinboardData',JSON.stringify(localStorageArray));
			}
		});

		var shownArticles = new Array();

		var sponsoredQueue = (new function() {
			var cls = this;
			cls.queueLoading = false;
			cls.outbrainLoading = false;
			cls.outbrainQueue = new Array();

			cls.init = function() {
				if (localStorageSupported) {
					cls.sponsoredTileInfo = $.parseJSON(localStorage.getItem('incSponsoredTileInfo'));
				}
				else {
					var incSponsoredTileInfo = new Object();
					incSponsoredTileInfo.queue = new Array();
					incSponsoredTileInfo.seen = new Array();
					cls.sponsoredTileInfo = incSponsoredTileInfo;
				}
				cls.loadQueue();
				cls.loadOutbrain();
			}

			cls.loadQueue = function() {
				if (!cls.queueLoading) {
					cls.queueLoading = true;

					var restURL = application_url+'rest/sponsoredtiles';
					if (typeof adinfo.channelArray.topid !== 'undefined') {
						restURL += '/'+adinfo.channelArray.topid;
					}

					$.ajax({
						dataType: 'json',
						url: restURL
					})
					.done(function(data) {
						//cls.sponsoredTileInfo.queue = cls.sponsoredTileInfo.queue.concat(data);
						cls.sponsoredTileInfo.queue = data;

						cls.queueLoading = false;
					})
					.fail(function(data) {
						cls.queueLoading = false;
					});
				}
			}

			cls.processOutbrain = function(json) {
				for (var i = 0; i < json.doc.length; i++) {
					var newOutbrainItem = json.doc[i];
					newOutbrainItem.cellclass = 'outbrainNew';
					newOutbrainItem.cellwidth = 1;

					cls.outbrainQueue.push(newOutbrainItem);
				}
				cls.outbrainLoading = false;
			}

			cls.loadOutbrain = function() {
				if (!cls.outbrainLoading) {
					cls.outbrainLoading = true;
					OBR.extern.callRecs({permalink:baseURL,widgetId:'AD_2'},cls.processOutbrain);
				}
			}

			cls.getNext = function() {
				var next = cls.sponsoredTileInfo.queue.shift();
				if (typeof next !== 'undefined' && _.indexOf(shownArticles,next.id) != -1) {
					return cls.getNext();
				}

				if (typeof next !== 'undefined') {
					next.cellclass = 'normal';
					next.cellwidth = 1;
				}
				else {
					if (cls.outbrainQueue.length > 0) {
						next = cls.outbrainQueue.shift();
						next.cellclass = 'outbrainNew';
						next.cellwidth = 1;
					}

					if (!cls.outbrainLoading && cls.outbrainQueue.length < 10) {
						cls.loadOutbrain();
					}
				}

				return next;
			}

			cls.recordSeen = function() {

			}
		});
		$(function(){ sponsoredQueue.init(); });
	</script>
	

