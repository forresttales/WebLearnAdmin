<!DOCTYPE html>
<html>
    <head>
		<meta charset="utf-8">

  		<title><%= full_title(yield(:title)) %></title> 
  		
    	<%= stylesheet_link_tag "application", media: "all", "data-turbolinks-track" => true %>
    	  		
   		<%= javascript_include_tag "application", "data-turbolinks-track" => true %>    		   		
    	
		<link rel="icon" type="image/png" href="/assets/logo-lc-round-fav.png" />
    	  		
			
	  	<%= csrf_meta_tags %>
	  	
  		<%= include_gon %>
	  	
	  	<%= render 'layouts/shim' %>  


    </head>
    <body>
    	
    	
		<%= render 'layouts/header' %>

		 		
		<div style="width: 1200px; margin-right: auto; margin-left: auto; background: #fff;">

    		<%= render 'layouts/header_admin' %>

    		<%= render 'layouts/header_title_admin' %>
    		
    		<%= render 'layouts/flash_alert' %>
						
			<%= yield %> 

			<%= render 'layouts/footer' %>
			
		</div>

		<%= debug(params) if Rails.env.development? %>
    	
    </body>
</html>


